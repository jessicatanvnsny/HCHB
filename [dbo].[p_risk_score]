							when  SUBSTRING(a.ceoa_answer, 1, 3) ='S61' then 1
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='S62' then 1
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='S70' then 1
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='S71' then 1
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='S72' then 1
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='S73' then 1
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='S76' then 1
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='S80' then 1
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='S81' then 1
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='S82' then 1
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='S83' then 1
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='S86' then 1
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='S88' then 1
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='S89' then 1
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='S90' then 1
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='S91' then 1
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='S92' then 1
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='S93' then 1
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='S98' then 1
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='S99' then 1
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='T14' then 1
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='T20' then 1
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='T21' then 1
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='T22' then 1
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='T23' then 1
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='T24' then 1
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='T25' then 1
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='T38' then 1
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='T45' then 1
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='T78' then 1
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='T79' then 1
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='T80' then 1
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='T81' then 1
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='T82' then 1
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='T83' then 1
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='T84' then 1
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='T85' then 1
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='T86' then 1
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='T87' then 1
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='Z21' then 1
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='Z42' then 1
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='Z43' then 1
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='Z45' then 1
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='Z46' then 1
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='Z47' then 1
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='Z48' then 1
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='Z60' then 1
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='Z68' then 1
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='Z72' then 1
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='Z74' then 1
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='Z79' then 1
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='Z85' then 1
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='Z86' then 1
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='Z87' then 1
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='Z89' then 1
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='Z90' then 1
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='Z91' then 1
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='Z93' then 1
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='Z94' then 1
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='Z95' then 1
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='Z96' then 1
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='Z98' then 1
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='Z99' then 1
						ELSE 0 END AS RESP_VAL
					from #risk_score
							join HCHB.dbo.CLIENT_EPISODE_OASIS  on RS_CFCEVID = CEO_CEVID -- carry forward id 
							join HCHB.DBO.CLIENT_EPISODE_OASIS_ANSWERS A
							on ceo_id = ceoa_ceoid
								  join SHARED_REFERENCE.oasis.OASIS_MOS O on ceoa_omid=om_id
					   WHERE -- om_formlocation LIKE '%M1023%' 
									om_formlocation in  ('M1021_PRIMARY_DIAG_ICD', 'M1023_OTH_DIAG1_ICD', 'M1023_OTH_DIAG2_ICD', 'M1023_OTH_DIAG3_ICD', 'M1023_OTH_DIAG4_ICD','M1023_OTH_DIAG5_ICD',
												'M1017_CHGREG_ICD1','M1017_CHGREG_ICD2','M1017_CHGREG_ICD3','M1017_CHGREG_ICD4','M1017_CHGREG_ICD5','M1017_CHGREG_ICD6',
											'M1025_OPT_DIAG_ICD_A3','M1025_OPT_DIAG_ICD_A4','M1025_OPT_DIAG_ICD_B3','M1025_OPT_DIAG_ICD_B4','M1025_OPT_DIAG_ICD_C3',
											'M1025_OPT_DIAG_ICD_C4','M1025_OPT_DIAG_ICD_D3','M1025_OPT_DIAG_ICD_D4','M1025_OPT_DIAG_ICD_E3','M1025_OPT_DIAG_ICD_E4',
											'M1025_OPT_DIAG_ICD_F3','M1025_OPT_DIAG_ICD_F4',
											'M1011_14_DAY_INP1_ICD','M1011_14_DAY_INP2_ICD','M1011_14_DAY_INP3_ICD','M1011_14_DAY_INP4_ICD','M1011_14_DAY_INP5_ICD',
											'M1011_14_DAY_INP6_ICD')
								and ceoa_answer != ''
						--and  rs_ceo_cevid = 19430 -- temporary for testing drop later
				) DX
				where dx.factor is not null
				group by rs_epiid, ceo_cevid, rs_astrsn, factor
				UNION --=============================== Broader Codigns for BP3  ==========================================================
				-- carry forward not required for these defitions at this time
				-- select ceo_cevid, factor, factor as parameter, MAX(RESP_VAL) AS RESP_VAL, MAX(2) AS cf_priority
				select  rs_epiid,ceo_cevid,rs_astrsn
								, UPPER(factor) AS factor
								, UPPER(factor) as parameter
								, MAX(RESP_VAL) AS factor_level
								, MAX(RESP_VAL) AS resp_val
								, 1 as cf_priority
				FROM (
						SELECT rs_epiid, ceo_cevid, rs_astrsn,ceoa_answer
										,  case 
										when om_formlocation = 'M1021_PRIMARY_DIAG_ICD' AND SUBSTRING(a.ceoa_answer, 1, 3) in ('I21', 'I22') then 'DX_PRIM_AMI_B'  
										when om_formlocation in ('M1023_OTH_DIAG1_ICD', 'M1023_OTH_DIAG2_ICD', 'M1023_OTH_DIAG3_ICD', 'M1023_OTH_DIAG4_ICD','M1023_OTH_DIAG5_ICD','M1023_OTH_DIAG6_ICD') AND SUBSTRING 
(a.ceoa_answer, 1, 3) in('I21','I22')    then 'DX_SEC_AMI_B'
													when om_formlocation = 'M1011_14_DAY_INP1_ICD'  AND SUBSTRING(a.ceoa_answer, 1, 3) in ('I21', 'I22') then 'IPDX_PRIM_AMI_B'  
										when om_formlocation in ('M1011_14_DAY_INP1_ICD','M1011_14_DAY_INP2_ICD', 'M1011_14_DAY_INP3_ICD','M1011_14_DAY_INP4_ICD', 'M1011_14_DAY_INP5_ICD','M1011_14_DAY_INP6_ICD')
																AND SUBSTRING(a.ceoa_answer, 1, 3) in('I21','I22','I25')    then 'IPDX_ANY_AMI_B'
													when om_formlocation = 'M1021_PRIMARY_DIAG_ICD'  AND SUBSTRING(a.ceoa_answer, 1, 3) = 'I50' then 'DX_PRIM_CHF_B'
													when om_formlocation = 'M1011_14_DAY_INP1_ICD'  AND SUBSTRING(a.ceoa_answer, 1, 3) = 'I50' then 'IPDX_PRIM_CHF_B'
													when om_formlocation in ('M1021_PRIMARY_DIAG_ICD', 'M1023_OTH_DIAG1_ICD', 'M1023_OTH_DIAG2_ICD', 'M1023_OTH_DIAG3_ICD',  
'M1023_OTH_DIAG4_ICD','M1023_OTH_DIAG5_ICD','M1023_OTH_DIAG6_ICD')
																AND SUBSTRING(a.ceoa_answer, 1, 3)  in('I13','I11','I09') then 'DX_OTH_HF_B' 
													when  om_formlocation in ('M1011_14_DAY_INP1_ICD','M1011_14_DAY_INP2_ICD',  
'M1011_14_DAY_INP3_ICD','M1011_14_DAY_INP4_ICD','M1011_14_DAY_INP5_ICD','M1011_14_DAY_INP6_ICD') 
																AND SUBSTRING(a.ceoa_answer, 1, 3) in('I13','I11','I09')   then  'IPDX_OTH_HF_B'          
								ELSE NULL END AS factor
								,  case 
										when om_formlocation = 'M1021_PRIMARY_DIAG_ICD' AND SUBSTRING(a.ceoa_answer, 1, 3) in ('I21', 'I22') then 1
										when om_formlocation in ('M1023_OTH_DIAG1_ICD', 'M1023_OTH_DIAG2_ICD', 'M1023_OTH_DIAG3_ICD', 'M1023_OTH_DIAG4_ICD','M1023_OTH_DIAG5_ICD','M1023_OTH_DIAG6_ICD') AND SUBSTRING 
(a.ceoa_answer, 1, 3) in('I21','I22') then 1
													when om_formlocation = 'M1011_14_DAY_INP1_ICD'  AND SUBSTRING(a.ceoa_answer, 1, 3) in ('I21', 'I22') then 1
										when om_formlocation in ('M1011_14_DAY_INP1_ICD','M1011_14_DAY_INP2_ICD', 'M1011_14_DAY_INP3_ICD','M1011_14_DAY_INP4_ICD', 'M1011_14_DAY_INP5_ICD','M1011_14_DAY_INP6_ICD')
																AND SUBSTRING(a.ceoa_answer, 1, 3) in('I21','I22','I25')    then 1
													when om_formlocation = 'M1021_PRIMARY_DIAG_ICD'  AND SUBSTRING(a.ceoa_answer, 1, 3) = 'I50' then 1
													when om_formlocation = 'M1011_14_DAY_INP1_ICD'  AND SUBSTRING(a.ceoa_answer, 1, 3) = 'I50' then 1
													when om_formlocation in ('M1021_PRIMARY_DIAG_ICD', 'M1023_OTH_DIAG1_ICD', 'M1023_OTH_DIAG2_ICD', 'M1023_OTH_DIAG3_ICD',  
'M1023_OTH_DIAG4_ICD','M1023_OTH_DIAG5_ICD','M1023_OTH_DIAG6_ICD')
																AND SUBSTRING(a.ceoa_answer, 1, 3)  in('I13','I11','I09') then 1
													when  om_formlocation in ('M1011_14_DAY_INP1_ICD','M1011_14_DAY_INP2_ICD',  
'M1011_14_DAY_INP3_ICD','M1011_14_DAY_INP4_ICD','M1011_14_DAY_INP5_ICD','M1011_14_DAY_INP6_ICD') 
																AND SUBSTRING(a.ceoa_answer, 1, 3) in('I13','I11','I09')   then  1   
								ELSE NULL END  RESP_VAL
					from #risk_score
										join HCHB.dbo.CLIENT_EPISODE_OASIS  on rs_ceo_cevid = ceo_cevid
										join HCHB.DBO.CLIENT_EPISODE_OASIS_ANSWERS A
										on ceo_id = ceoa_ceoid
												join SHARED_REFERENCE.oasis.OASIS_MOS O on ceoa_omid=om_id
							WHERE -- om_formlocation LIKE '%M1023%' 
													om_formlocation in  
																			('M1021_PRIMARY_DIAG_ICD', 'M1023_OTH_DIAG1_ICD', 'M1023_OTH_DIAG2_ICD', 'M1023_OTH_DIAG3_ICD',  
'M1023_OTH_DIAG4_ICD','M1023_OTH_DIAG5_ICD',
																				 
'M1017_CHGREG_ICD1','M1017_CHGREG_ICD2','M1017_CHGREG_ICD3','M1017_CHGREG_ICD4','M1017_CHGREG_ICD5','M1017_CHGREG_ICD6',
																				 
'M1025_OPT_DIAG_ICD_A3','M1025_OPT_DIAG_ICD_A4','M1025_OPT_DIAG_ICD_B3','M1025_OPT_DIAG_ICD_B4','M1025_OPT_DIAG_ICD_C3',
																				 
'M1025_OPT_DIAG_ICD_C4','M1025_OPT_DIAG_ICD_D3','M1025_OPT_DIAG_ICD_D4','M1025_OPT_DIAG_ICD_E3','M1025_OPT_DIAG_ICD_E4',
																				'M1025_OPT_DIAG_ICD_F3','M1025_OPT_DIAG_ICD_F4',
																				 
'M1011_14_DAY_INP1_ICD','M1011_14_DAY_INP2_ICD','M1011_14_DAY_INP3_ICD','M1011_14_DAY_INP4_ICD','M1011_14_DAY_INP5_ICD','M1011_14_DAY_INP6_ICD'
																)
											and ceoa_answer != '' 
																AND rs_astrsn in (1,3)
															) dx2
				where factor is not null                                        
				group by rs_epiid,ceo_cevid,rs_astrsn, factor ) AA
			) AAA
			WHERE srt_cf_priority = 1
				;

	OPEN C
	 FETCH NEXT FROM C INTO @RS_EPIID , @pRS_CEO_CEVID, @RS_ASTRSN , @FACTOR_ID ,@PARAMETER , @RESP_NUM ,@RESP_VAL ;

	 WHILE @@FETCH_STATUS = 0
	 begin
	
				 INSERT INTO @RISK_SCORE_X VALUES( @RS_EPIID , @pRS_CEO_CEVID, @RS_ASTRSN , @FACTOR_ID ,@PARAMETER , @RESP_NUM ,@RESP_VAL )

			   -- what you want to do in catch
			   declare @error int, @message varchar(4000), @xstate int;
			   select @error = ERROR_NUMBER(),
					 @message = ERROR_MESSAGE(), @xstate = XACT_STATE();
			
			FETCH NEXT FROM C INTO @RS_EPIID , @pRS_CEO_CEVID, @RS_ASTRSN , @FACTOR_ID ,@PARAMETER , @RESP_NUM ,@RESP_VAL ;

	 end
	 CLOSE C;
	-- 关闭游标
    -- 释放游标
    DEALLOCATE C;


	

     truncate table [dbo].[risk_score]

	---hospitalization  bp3 identification
	 insert into  dbo.risk_score 
	       (rs_epiid,rs_ceo_cevid,rs_astrsn,rs_pred_val,rs_lmid,rs_lrlid) 	
          select rs_epiid,rs_ceo_cevid, lrsb_astrsn,
		       1/ (1 + exp(-1*sum(lrsb_coef*x.resp_val ))) as pred_val,lrsb_lmid,
               case 
			      when 1/ (1 + exp(-1*sum(lrsb_coef*x.resp_val )))  >@hosphigh then 25103
				  when 1/ (1 + exp(-1*sum(lrsb_coef*x.resp_val )))  <@hosplow  then 25101
				  else 25102
			   end as ind
          from dbo.lu_risk_score_beta 
	         join  @RISK_SCORE_X x
		          on lrsb_parameter = parameter 
				     and lrsb_astrsn = rs_astrsn
          where lrsb_lmid='25100'  
          group by rs_epiid,rs_ceo_cevid ,lrsb_astrsn,lrsb_lmid
     union
          select rs_epiid,rs_ceo_cevid, lrsb_astrsn,
						   1/ (1 + exp(-1*sum(lrsb_coef*x.resp_val )))  as coef2,lrsb_lmid,
						   case 
							  when 1/ (1 + exp(-1*sum(lrsb_coef*x.resp_val )))  > @bp3identf then 25301 
							  else -1
						   end as ind
		  from dbo.lu_risk_score_beta 
						 join @RISK_SCORE_X  x
							  on lrsb_parameter  = parameter 
								 and lrsb_astrsn = rs_astrsn
						-- join medicare_epiid on m_epiid = rs_epiid
		  where lrsb_lmid='25300' 
		  group by rs_epiid,rs_ceo_cevid ,lrsb_astrsn,lrsb_lmid
     
	 --bp3 hospitalization 
	  insert into dbo.risk_score 
	       (rs_epiid,rs_ceo_cevid,rs_astrsn,rs_pred_val,rs_lmid,rs_lrlid) 	
          select  rs_epiid,rs_ceo_cevid,rs_astrsn,rs_pred_val, 25200 as rs_lmid,
		       case 
			      when  rs_pred_val  > @bp3hosphigh then 25203
				  when  rs_pred_val  < @bp3hosplow  then 25201
				  else 25202
			   end as ind
		   from dbo.risk_score 
		   where rs_lmid='25100'  
	 
		 
	 ---bp3 identification

			 /*
