			 select distinct epi_id
			 from HCHB.dbo.CLIENT_EPISODE_FS
				  join HCHB.dbo.client_episodes_all on cefs_epiid = epi_id
					  join HCHB.dbo.PAYOR_sources on ps_ptid = cefs_ptid 
			 where --ps_ptid = 57
			       ps_freq = 5
				   and ps_desc <> 'zz system use only - do not use'
				   ) 
				   
		insert into  dbo.risk_score (rs_epiid,rs_ceo_cevid,rs_astrsn,rs_pred_val,rs_lmid,rs_lrlid)
			 select rs_epiid,rs_ceo_cevid, lrsb_astrsn,
