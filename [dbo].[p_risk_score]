		  WHERE rs_lmid = 25300 and rs_lrlid = 25301
		  group by x.rs_epiid,x.rs_ceo_cevid ,lrsb_astrsn,lrsb_lmid
		*/


	  IF OBJECT_ID('tempdb..#evacuation_zone') IS NOT NULL DROP TABLE #evacuation_zone
	  CREATE TABLE #evacuation_zone (ez_epiid int,ez_ceslid int)

		 
		 ;with newest_date (ncesl_id,ncesl_cslid,ncesl_epiid,newest_date)
		 as (select cesl_id,cesl_cslid,cesl_epiid ,
			 case when cesl.cesl_insertdate > ar.ar_datemodified
				  then cesl.cesl_insertdate 
				  when cesl.cesl_insertdate < ar.ar_datemodified
				  then ar.ar_datemodified
				  when ar.ar_datemodified is null
				  then cesl.cesl_insertdate 	  
			  end as newest_date
			 from HCHB.dbo.client_episode_service_locations cesl with(nolock)
			 left join HCHB.dbo.auditreport_client_episode_service_locations ar with(nolock) on ar.ar_cesl_id = cesl.cesl_id
			 ) 
		 insert into #evacuation_zone (ez_ceslid,ez_epiid)
		 select distinct
			 cesl_id
			,cesl_epiid
		 from HCHB.dbo.client_episode_service_locations cesl  join newest_date
			  on  cesl_id = ncesl_id and 
				  cesl_cslid = ncesl_cslid and
				  cesl_epiid = ncesl_epiid
		 where  DATEDIFF(day,newest_date, getdate()) < @Param1
			union
		 SELECT distinct cesl_id,cesl_epiid
		 FROM HCHB.DBO.CLIENT_EPISODE_SERVICE_LOCATIONS
		 where cesl_epiid  not in (select rsh_epiid from dbo.risk_score_history)
			  and cesl_id  not in (select rsh_ceslid from dbo.risk_score_history)
  
     ----#medicare_epiid episoede, frequency = 5
	  IF OBJECT_ID('tempdb..#medicare_epiid') IS NOT NULL DROP TABLE #medicare_epiid
	  CREATE TABLE #medicare_epiid (m_epiid int)

			 insert into #medicare_epiid (M_EPIID) 
			 select distinct epi_id
			 from HCHB.dbo.CLIENT_EPISODE_FS
				  join HCHB.dbo.client_episodes_all on cefs_epiid = epi_id
					  join HCHB.dbo.PAYOR_sources on ps_id = cefs_psid 
			 where --ps_ptid = 57
			       ps_freq = 5
				   and ps_desc <> 'zz system use only - do not use'
			
	 
	 ----log table loaded  		 
	  
	  truncate table [dbo].[risk_score_log]

	 -- hospitalization, bp3 indentification, bp3 hospitalization 
			;with bp3_epi (bp3_epiid,bp3_ceo_cevid) as
				   (
					select rs_epiid,rs_ceo_cevid
					from dbo.risk_score r
					WHERE rs_lmid = 25200
					   and EXISTS (
						select *
						from HCHB.dbo.CLIENT_EPISODE_CARE_TYPES
						WHERE rs_epiid=cect_epiid
							and cect_ctypeid =  25030 -- BP3
					)
					  or (rs_lmid = 25300 and rs_lrlid <> -1)
					) 
	insert into  dbo.risk_score_log
		(rsl_epiid,rsl_ceo_cevid,rsl_astrsn,rsl_lmid,rsl_lrlid,care_type,rsl_rsntid--,additional_note_text
		,rsl_agency_id)
			select rs_epiid,rs_ceo_cevid,rs_astrsn,rs_lmid,rs_lrlid ,null as care_type, lrsnt_id,
			@Param2  as rsl_agency_id
			from .dbo.risk_score 
			 join bp3_epi 
				on bp3_epiid = rs_epiid 
				   and bp3_ceo_cevid = rs_ceo_cevid
			 join #medicare_epiid
			       on m_epiid = rs_epiid
			 join [dbo].[lu_risk_level]
						   on lrl_id = rs_lrlid
						left join [dbo].[lu_risk_score_note_type]
						   on rs_lmid = lrsnt_lmid
			where rs_lmid = 25200
			union
			select rs_epiid,rs_ceo_cevid,rs_astrsn,rs_lmid,rs_lrlid ,
						  null as care_type,
						   lrsnt_id,
						 /*  case 
							  when rs_lmid = 25100 then (lrsnt_note_type_desc +': '+ risk_level_desc) -- +' Guidance:'+ ltrim(str(lrl_rn_visit_number)) + ' Nuring Visits')
							  when rs_lmid = 25200 then (lrsnt_note_type_desc +': '+ risk_level_desc)
						end as note,
						*/
						@Param2  as rsl_agency_id
					  from  dbo.[risk_score] 
						join [dbo].[lu_risk_level]
						   on lrl_id = rs_lrlid
						--join #medicare_epiid
			             --  on m_epiid = rs_epiid
						left join [dbo].[lu_risk_score_note_type]
						   on rs_lmid = lrsnt_lmid
					  WHERE rs_lrlid <> -1
							and rs_lmid = 25100
			union
			select rs_epiid,rs_ceo_cevid,rs_astrsn,rs_lmid,rs_lrlid ,
						   case
							  when rs_lmid = 25300 then 25030
						   end as care_type,
						   lrsnt_id,
						 /*  case 
							  when rs_lmid = 25100 then (lrsnt_note_type_desc +': '+ risk_level_desc) -- +' Guidance:'+ ltrim(str(lrl_rn_visit_number)) + ' Nuring Visits')
							  when rs_lmid = 25200 then (lrsnt_note_type_desc +': '+ risk_level_desc)
						end as note,
						*/
						@Param2  as rsl_agency_id
					  from  dbo.[risk_score] 
						join [dbo].[lu_risk_level]
						   on lrl_id = rs_lrlid
						join #medicare_epiid
			               on m_epiid = rs_epiid
						left join [dbo].[lu_risk_score_note_type]
						   on rs_lmid = lrsnt_lmid
					  WHERE rs_lrlid <> -1
							and rs_lmid = 25300


      --evacuation zone
	 insert into  dbo.risk_score_log
	    (rsl_epiid,rsl_ceslid, rsl_lmid,rsl_lrlid,rsl_rsntid,rsl_agency_id)
	     select s.ez_epiid,s.ez_ceslid,s.ez_lmid,s.ez_lrlid,lrsnt_id, @Param2  as rsl_agency_id
		 from #evacuation_zone e 
		   join stg.evacuation_zone s
		        on e.ez_epiid = s.ez_epiid and e.ez_ceslid = s.ez_ceslid
		   left join  [dbo].[lu_risk_score_note_type]
                on ez_lmid = lrsnt_lmid
		  where ez_lrlid is not null
     
     

	 ---history table arcive 
	/*
			  insert into [dbo].[risk_score_history]
			     ([rsh_epiid],rsh_ceo_cevid,rsh_astrsn,rsh_lmid,rsh_pred_val,rsh_lrlid,care_type,rsh_rsntid,rsh_agency_id)
			select rs_epiid, rs_ceo_cevid,rs_astrsn,rs_lmid,rs_pred_val, rs_lrlid ,null as care_type, lrsnt_id,
			@Param2  as rsl_agency_id
			from dbo.risk_score 
			 join [dbo].[lu_risk_level]
						   on lrl_id = rs_lrlid
						left join [dbo].[lu_risk_score_note_type]
						   on rs_lmid = lrsnt_lmid
			where rs_lmid = 25200
			union
			select rs_epiid,rs_ceo_cevid,rs_astrsn,rs_lmid,rs_pred_val,rs_lrlid ,
						   case
							  when rs_lmid = 25300 then 25030
						   end as care_type,
						   lrsnt_id,
						 /*  case 
							  when rs_lmid = 25100 then (lrsnt_note_type_desc +': '+ risk_level_desc) -- +' Guidance:'+ ltrim(str(lrl_rn_visit_number)) + ' Nuring Visits')
							  when rs_lmid = 25200 then (lrsnt_note_type_desc +': '+ risk_level_desc)
						end as note,
						*/
						@Param2  as rsl_agency_id
					  from  dbo.[risk_score] 
						join [dbo].[lu_risk_level]
						   on lrl_id = rs_lrlid
						left join [dbo].[lu_risk_score_note_type]
						   on rs_lmid = lrsnt_lmid
					  WHERE rs_lrlid <> -1
							and rs_lmid in (25100,25300)
		

  	 --    insert into [dbo].[risk_score_history]
	--		   ([rsh_epiid],rsh_ceslid,rsh_lmid,rsh_lrlid,rsh_rsntid,rsh_agency_id)
	----			 select rsl_epiid,rsl_ceslid,rsl_lmid,rsl_lrlid,rsl_rsntid,@Param2  as rsl_agency_id
	--			 from dbo.risk_score_log
	--			 where rsl_lmid = 25400
	*/
         

	    merge into risk_score_history tgt
		using (select  rs_epiid, rs_ceo_cevid,rs_astrsn,rs_lmid,rs_pred_val, rs_lrlid , care_type, rsl_agency_id, additional_note_text,rsl_rsntid
				,rs.upd_ts,rs.crt_id,rs.crt_ts,rs.upd_id
			   from dbo.risk_score rs left join dbo.risk_score_log rsl on rs_epiid = rsl_epiid and rs_ceo_cevid = rsl_ceo_cevid and rs_lmid = rsl_lmid) src
		on( tgt.rsh_epiid = src.rs_epiid and
			tgt.rsh_ceo_cevid = src.rs_ceo_cevid and
			tgt.rsh_astrsn = src.rs_astrsn and
			tgt.rsh_lmid  = src.rs_lmid  and
			tgt.rsh_lrlid = src.rs_lrlid and
			tgt.rsh_pred_val = src.rs_pred_val
            )
		when not matched then 
		 insert (rsh_epiid
				,rsh_ceo_cevid
				,rsh_astrsn
				 ,rsh_lmid 
				 ,rsh_pred_val
				 ,rsh_lrlid 
				 ,care_type
				 ,rsh_agency_id
				 ,rsh_additional_note_text
				 ,rsh_rsntid
				 ,upd_ts
				 ,crt_id
				 ,crt_ts
				 ,upd_id)
		 values( src.rs_epiid, src.rs_ceo_cevid,src.rs_astrsn,
				 src.rs_lmid,src.rs_pred_val, src.rs_lrlid , 
				 src.care_type, src.rsl_agency_id, 
				 src.additional_note_text,rsl_rsntid
				,src.upd_ts,src.crt_id,src.crt_ts,src.upd_id)
				;

	   	    merge into dbo.[risk_score_history] tgt
	     	using (select ez_epiid,ez_ceslid,ez_lmid, ez_lrlid,lrsnt_id,rsl_agency_id
		               ,rsl.upd_ts,rsl.upd_id,rsl.crt_ts,rsl.crt_id
		        from stg.evacuation_zone 
				    left join dbo.risk_score_log rsl
					   on ez_epiid = rsl_epiid and ez_ceslid = rsl_ceslid and ez_lmid = rsl_lmid
		        	left join  [dbo].[lu_risk_score_note_type]
                       on ez_lmid = lrsnt_lmid) src
	    	on (tgt.rsh_epiid = src.ez_epiid and 
		        tgt.rsh_ceslid = src.ez_ceslid and
		    	tgt.rsh_lmid = src.ez_lmid and
			    tgt.rsh_lrlid = src.ez_lrlid)
		    when not matched then 
		      insert (rsh_epiid,rsh_ceslid,rsh_lmid,rsh_lrlid,rsh_agency_id, rsh_rsntid,upd_ts,upd_id,crt_ts,crt_id)
			  values (ez_epiid ,ez_ceslid,ez_lmid ,ez_lrlid,rsl_agency_id,lrsnt_id,upd_ts,upd_id,crt_ts,crt_id);
        
		

		    ;with flag (frsh_id,frsh_epiid,frsh_ceo_cevid,frsh_astrsn,frsh_ceslid,frsh_lmid,crt_ts,fflag) as
			(
			SELECT rsh_id,rsh_epiid,rsh_ceo_cevid,rsh_astrsn,rsh_ceslid,rsh_lmid,crt_ts
				   ,rank()over (partition by rsh_epiid,rsh_ceo_cevid,rsh_astrsn,rsh_ceslid,rsh_lmid
						  order by crt_ts desc) as flag
						 FROM [risk_score_history] 
						 )
		merge into  [risk_score_history] tgt
		using (select frsh_id,frsh_epiid,frsh_ceo_cevid,frsh_astrsn,frsh_ceslid,frsh_lmid,fflag ,crt_ts
			   from  flag ) src
		on (frsh_id = rsh_id and frsh_epiid = rsh_epiid )
		when matched then 
		 update set tgt.flag = src.fflag;
		 
			

			
								

	SELECT @iErrorCode=@@ERROR	 
END
GO


