-- =============================================
-- Author:		<Jessica>
-- Create date: <2018-04-12>
-- Description:	<risk model for hospitalization, bp3 identification, bp3 hospitalization >
-- =============================================
CREATE  PROCEDURE [dbo].[p_risk_score]
	-- Add the parameters for the stored procedure here
	@Param1 int = 7,
	@Param2 INT = 2684
AS
BEGIN 
 	-- SET NOCOUNT ON added to prevent extra result sets from
	-- interfering with SELECT statements.
	SET NOCOUNT ON;

    -- Insert statements for procedure here	  
	DECLARE
	@iErrorCode int ,
    @hosphigh float,
	@hosplow float,
	@bp3hosphigh float,
	@bp3hosplow float,
	@bp3identf float

	   set @hosphigh   = (select high from dbo.lu_risk_level_range where lrlr_lrlid = 25102)
       set @hosplow    = (select low  from dbo.lu_risk_level_range where lrlr_lrlid = 25102)
	   set @bp3hosphigh= (select high from dbo.lu_risk_level_range where lrlr_lrlid = 25202)
       set @bp3hosplow = (select low  from dbo.lu_risk_level_range where lrlr_lrlid = 25202)
	   set @bp3identf  = (select low  from dbo.lu_risk_level_range where lrlr_lrlid = 25301)

	IF OBJECT_ID('tempdb..#risk_score') IS NOT NULL DROP TABLE #risk_score
	CREATE TABLE #risk_score (rs_epiid int,rs_ceo_cevid int,rs_cfcevid INT,rs_astrsn int
	                       -- ,ceo_insertdate date,ceo_lastupdate date
							)

   
        ;with newest_date ( nceoa_id,nceoa_ceoid,newest_date) as(
        select   ceoa_id
                ,ceoa_ceoid,
		    case when  ceoa_insertdate > ar_datemodified
		         then ceoa_insertdate
			     when ceoa_insertdate < ar_datemodified
		    	 then ar_datemodified
		    	 when ar_datemodified is null
			     then ceoa_insertdate
	         end as newest_date
         from HCHB.dbo.CLIENT_EPISODE_OASIS_ANSWERS 
 　　　　　 　left join HCHB.dbo.AUDITREPORT_CLIENT_EPISODE_OASIS_ANSWERS
               on ar_ceoa_id = ceoa_id and
			      ar_ceoa_ceoid = ceoa_ceoid 
           )
    insert into #risk_score (rs_epiid,rs_ceo_cevid,rs_cfcevid,rs_astrsn)
       select ceo_epiid,ceo_cevid, 
	          LAG (ceo_cevid, 1, 0) OVER (partition by ceo_epiid order by CEV_VISITDATE ) AS rs_cfcevid,
			  cast(ceoa_OASISAnswer as int) as  rs_astrsn 
       from newest_date 
        join HCHB.dbo.CLIENT_EPISODE_OASIS_ANSWERS
           on nceoa_id = ceoa_id and nceoa_ceoid = ceoa_ceoid
	    join HCHB.dbo.CLIENT_EPISODE_OASIS 
	       on ceo_id = ceoa_ceoid
		join HCHB.dbo.CLIENT_EPISODE_VISITS 
		   on cev_id=ceo_cevid
	    join SHARED_REFERENCE.oasis.OASIS_MOS 
	       on ceoa_omid=om_id
       where om_monumber='M0100' AND ceoa_OASISAnswer IN ('01','03','09')
           and  DATEDIFF(day,newest_date, getdate()) < @Param1
/*		 union
	   select  ceo_epiid 
			 , ceo_cevid
			 ,LAG (ceo_cevid, 1, 0) OVER (partition by ceo_epiid order by CEV_VISITDATE ) AS rs_cfcevid
			 , cast(ceoa_OASISAnswer as int) as  rs_astrsn 
			-- , ceo_insertdate 
			-- , ceo_lastupdate
			-- , DATEDIFF(DAY, convert(date, ceo_lastupdate), convert(date, GETDATE())) as time_flag
       from HCHB.dbo.client_episode_oasis 
           join  HCHB.dbo.CLIENT_EPISODE_OASIS_ANSWERS on ceo_id = ceoa_ceoid
		   join HCHB.dbo.CLIENT_EPISODE_VISITS on cev_id=ceo_cevid
           join SHARED_REFERENCE.oasis.OASIS_MOS on ceoa_omid=om_id
       where om_monumber='M0100' AND cast(ceoa_OASISAnswer as int) IN (1,3,9)
             and (DATEDIFF(DAY, convert(date, ceo_lastupdate), convert(date, GETDATE())) < 100
		         or DATEDIFF(DAY, convert(date, ceo_insertdate), convert(date, GETDATE())) < 100)
*/
	   union
	   select  ceo_epiid 
			 , ceo_cevid
			 , LAG (ceo_cevid, 1, 0) OVER (partition by ceo_epiid order by CEV_VISITDATE ) AS rs_cfcevid
			 , cast(ceoa_OASISAnswer as int) as  rs_astrsn 
			-- , ceo_insertdate 
			-- , ceo_lastupdate
			-- , DATEDIFF(DAY, convert(date, ceo_lastupdate), convert(date, GETDATE())) as time_flag
	   from HCHB.dbo.client_episode_oasis 
           join  HCHB.dbo.CLIENT_EPISODE_OASIS_ANSWERS on ceo_id = ceoa_ceoid
		   join HCHB.dbo.CLIENT_EPISODE_VISITS on cev_id=ceo_cevid
           join SHARED_REFERENCE.oasis.OASIS_MOS on ceoa_omid=om_id
       where om_monumber='M0100' AND ceoa_OASISAnswer IN ('01','03','09')
	         and (ceo_epiid  not in (select rsh_epiid from dbo.risk_score_history)
		  	       or ceo_cevid  not in (select rsh_ceo_cevid from dbo.risk_score_history))



     
DECLARE @RISK_SCORE_X TABLE
(
	RS_EPIID INT, RS_CEO_CEVID INT, RS_ASTRSN INT, FACTOR_ID VARCHAR(50), PARAMETER VARCHAR(50),RESP_NUM INT, RESP_VAL INT
);
DECLARE @RS_EPIID AS INT, @pRS_CEO_CEVID AS INT, @RS_ASTRSN AS INT, @FACTOR_ID AS VARCHAR(50),@PARAMETER AS VARCHAR(50), @RESP_NUM AS INT,@RESP_VAL AS INT;

DECLARE C CURSOR FAST_FORWARD /* read only, forward only */
FOR    			
            select rs_epiid , rs_ceo_cevid, rs_astrsn, factor_id, parameter, resp_num, resp_val
			FROM (
			select aa.*
					, ROW_NUMBER ()   OVER (partition by rs_ceo_cevid, parameter order by cf_priority ) AS srt_cf_priority -- add this field
			from (
				SELECT rs_epiid , rs_ceo_cevid, rs_astrsn
								,'INTERCEPT' as factor_id
								, 'INTERCEPT' as parameter
								, 1 as resp_num
								, 1 as resp_val
					, 1 as cf_priority
				FROM  #risk_score
       UNION --==================================== OASIS Items  ========================================================================
       select rs_epiid,rs_ceo_cevid, rs_astrsn,
              om_formlocation as factor,
                     case when  om_monumber in ('M1311') then om_formlocation -- continuous
                           else  concat(om_formlocation, factor_level)  -- factor
                     end as parameter
              , factor_level
              , resp_val  
              , 1 as cf_priority
       from ( 
       SELECT  rs_epiid, rs_ceo_cevid, rs_astrsn, om_monumber, ceo_epiid, ceo_id, om_formlocation, ceoa.ceoa_OASISAnswer, -- om_type, om_atype, 
                           case 
                                  when ceoa.ceoa_OASISAnswer in ('^') then                                  
                                         (caSE 
                                               when  om_monumber in ('M1311') then 0 
                                               when  om_monumber in ('M1021', 'M1023') then 0
                                          else 1 END)
                                  --when ceoa_OASISAnswer not in ('UK', 'NA', '^') then ISNULL(cast(ceoa_OASISAnswer as int),0)                              
                                  when ceoa.ceoa_OASISAnswer in ('UK') then -1 
                                  when ceoa.ceoa_OASISAnswer in ('NA') then -2
                                  when om_type in (3, 4) AND  ISNUMERIC (ceoa_OASISAnswer) = 1  and om_atype is NULL then ISNULL(cast(ceoa.ceoa_OASISAnswer as int),0) 
								  when om_monumber in ('M1100', 'M0110', 'M0080', 'M0069') then ISNULL(cast(ceoa.ceoa_OASISAnswer as int),0) 
                                  when om_formlocation IN ('M1021_PRIMARY_DIAG_SEVERITY', 'M1023_OTH_DIAG1_SEVERITY', 'M1023_OTH_DIAG2_SEVERITY', 'M1023_OTH_DIAG3_SEVERITY', 'M1023_OTH_DIAG4_SEVERITY', 'M1023_OTH_DIAG5_SEVERITY') then ISNULL(cast(ceoa.ceoa_OASISAnswer as int),0) 
                                  else 0
                           end as factor_level
                           , case 
                                         when ceoa.ceoa_OASISAnswer in ('^') then                                           
										  (case 
                                                when  om_monumber in ('M1311') then 0 
                                                when  om_monumber in ('M1021', 'M1023') then 0
                                           else 1 END)
                                         when ceoa.ceoa_OASISAnswer in ('UK') then 1
                                         when ceoa.ceoa_OASISAnswer in ('NA') then 1
                                         when om.om_monumber in ('M1311') then ISNULL(cast(ceoa.ceoa_OASISAnswer as int),0)
                                         when om_type in (4) and om_atype is NULL then ISNULL(cast(ceoa.ceoa_OASISAnswer as int),0)  -- Ordinal and mark all that apply
                                         when om_type in (3) and om_atype is NULL then 1 -- Ordinal and mark all that apply
                                  -- when om_formlocation IN ('M1021_PRIMARY_DIAG_SEVERITY', 'M1023_OTH_DIAG1_SEVERITY', 'M1023_OTH_DIAG2_SEVERITY', 'M1023_OTH_DIAG3_SEVERITY', 'M1023_OTH_DIAG4_SEVERITY', 'M1023_OTH_DIAG5_SEVERITY') then ISNULL(cast(ceoa.ceoa_OASISAnswer as int),0) 
                                  else 1
                           end as resp_val
       from #risk_score
                     join HCHB.dbo.CLIENT_EPISODE_OASIS ceo on rs_ceo_cevid = ceo_cevid
                     join HCHB.dbo.CLIENT_EPISODE_OASIS_ANSWERS ceoa on (ceo.ceo_id = ceoa.ceoa_ceoid) 
                     join SHARED_REFERENCE.oasis.OASIS_MOS om on ceoa_omid=om_id 
       where SUBSTRING(om_formlocation, 1, 11) <> 'ITEM_FILLER'
              -- and substring(om_formlocation, 1, 5) in ('M0140', 'M1845')
              -- order by om_formlocation
                     ) prex
       --order by om_formlocation
       UNION --==================================== OASIS Items CF  ========================================================================
       select rs_epiid,rs_ceo_cevid, rs_astrsn,
              om_formlocation as factor,
                     case when  om_monumber in ('M1311') then om_formlocation -- continuous
                           else  concat(om_formlocation, factor_level)  -- factor
                     end as parameter
              , factor_level
              , resp_val  
              , 2 as cf_priority
       from ( 
SELECT  rs_epiid, rs_ceo_cevid, rs_astrsn, om_monumber, ceo_epiid, ceo_id, om_formlocation, ceoa.ceoa_OASISAnswer, -- om_type, om_atype, 
                           case 
                                  when ceoa.ceoa_OASISAnswer in ('^') then 
								         (caSE 
                                               when  om_monumber in ('M1311') then 0 
                                               when  om_monumber in ('M1021', 'M1023') then 0
                                          else 1 END)                           
                                  --when ceoa_OASISAnswer not in ('UK', 'NA', '^') then ISNULL(cast(ceoa_OASISAnswer as int),0)                              
                                  when ceoa.ceoa_OASISAnswer in ('UK') then -1 
                                  when ceoa.ceoa_OASISAnswer in ('NA') then -2
                                  when om_type in (3, 4) AND  ISNUMERIC (ceoa_OASISAnswer) = 1   and om_atype is NULL then ISNULL(cast(ceoa.ceoa_OASISAnswer as int),0) 
								  when om_monumber in ('M1100', 'M0110', 'M0080', 'M0069') then ISNULL(cast(ceoa.ceoa_OASISAnswer as int),0) 
                                  when om_formlocation IN ('M1021_PRIMARY_DIAG_SEVERITY', 'M1023_OTH_DIAG1_SEVERITY', 'M1023_OTH_DIAG2_SEVERITY', 'M1023_OTH_DIAG3_SEVERITY', 'M1023_OTH_DIAG4_SEVERITY', 'M1023_OTH_DIAG5_SEVERITY') then ISNULL(cast(ceoa.ceoa_OASISAnswer as int),0) 
                                  else 0
                           end as factor_level
                           , case 
                                         when ceoa.ceoa_OASISAnswer in ('^') then 
										 (case
                                               when  om_monumber in ('M1311') then 0 
                                               when  om_monumber in ('M1021', 'M1023') then 0
                                          else 1 END) 
                                         when ceoa.ceoa_OASISAnswer in ('UK') then 1
                                         when ceoa.ceoa_OASISAnswer in ('NA') then 1
                                         when om.om_monumber in ('M1311') then ISNULL(cast(ceoa.ceoa_OASISAnswer as int),0)
                                         when om_type in (4) and om_atype is NULL then ISNULL(cast(ceoa.ceoa_OASISAnswer as int),0)  -- Ordinal and mark all that apply
                                         when om_type in (3) and om_atype is NULL then 1 -- Ordinal and mark all that apply
                                  -- when om_formlocation IN ('M1021_PRIMARY_DIAG_SEVERITY', 'M1023_OTH_DIAG1_SEVERITY', 'M1023_OTH_DIAG2_SEVERITY', 'M1023_OTH_DIAG3_SEVERITY', 'M1023_OTH_DIAG4_SEVERITY', 'M1023_OTH_DIAG5_SEVERITY') then ISNULL(cast(ceoa.ceoa_OASISAnswer as int),0) 
                                  else 1
                           end as resp_val
       from #risk_score -- VNSNY_STAT_DEV.dbo.risk_score
                     join HCHB.dbo.CLIENT_EPISODE_OASIS ceo on rs_cfcevid = ceo_cevid
                     join HCHB.dbo.CLIENT_EPISODE_OASIS_ANSWERS ceoa on (ceo.ceo_id = ceoa.ceoa_ceoid) 
                     join SHARED_REFERENCE.oasis.OASIS_MOS om on ceoa_omid=om_id 
       where SUBSTRING(om_formlocation, 1, 11) <> 'ITEM_FILLER'
                     and rs_cfcevid <> 0 -- just to elminate extras
                     ) prex

				UNION --====================== DIAGNOSIS   =======================================================================
				select rs_epiid,ceo_cevid,rs_astrsn, factor, factor as parameter, MAX(RESP_VAL) AS factor_level, MAX(RESP_VAL) AS RESP_VAL, MAX(2) AS cf_priority
				FROM (
					SELECT  rs_epiid,ceo_cevid,rs_astrsn
							, 
						CASE  
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='A04' then 'DXX_A04'
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='A09' then 'DXX_A09'
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='A31' then 'DXX_A31'
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='A40' then 'DXX_A40'
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='A41' then 'DXX_A41'
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='A49' then 'DXX_A49'
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='A69' then 'DXX_A69'
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='B00' then 'DXX_B00'
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='B02' then 'DXX_B02'
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='B18' then 'DXX_B18'
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='B19' then 'DXX_B19'
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='B20' then 'DXX_B20'
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='B25' then 'DXX_B25'
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='B34' then 'DXX_B34'
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='B35' then 'DXX_B35'
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='B37' then 'DXX_B37'
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='B95' then 'DXX_B95'
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='B96' then 'DXX_B96'
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='B97' then 'DXX_B97'
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='C02' then 'DXX_C02'
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='C06' then 'DXX_C06'
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='C09' then 'DXX_C09'
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='C10' then 'DXX_C10'
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='C11' then 'DXX_C11'
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='C15' then 'DXX_C15'
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='C16' then 'DXX_C16'
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='C17' then 'DXX_C17'
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='C18' then 'DXX_C18'
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='C19' then 'DXX_C19'
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='C20' then 'DXX_C20'
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='C21' then 'DXX_C21'
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='C22' then 'DXX_C22'
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='C23' then 'DXX_C23'
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='C24' then 'DXX_C24'
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='C25' then 'DXX_C25'
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='C32' then 'DXX_C32'
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='C34' then 'DXX_C34'
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='C40' then 'DXX_C40'
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='C41' then 'DXX_C41'
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='C43' then 'DXX_C43'
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='C44' then 'DXX_C44'
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='C45' then 'DXX_C45'
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='C48' then 'DXX_C48'
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='C49' then 'DXX_C49'
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='C50' then 'DXX_C50'
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='C51' then 'DXX_C51'
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='C53' then 'DXX_C53'
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='C54' then 'DXX_C54'
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='C55' then 'DXX_C55'
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='C56' then 'DXX_C56'
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='C57' then 'DXX_C57'
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='C61' then 'DXX_C61'
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='C64' then 'DXX_C64'
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='C67' then 'DXX_C67'
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='C71' then 'DXX_C71'
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='C73' then 'DXX_C73'
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='C76' then 'DXX_C76'
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='C77' then 'DXX_C77'
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='C78' then 'DXX_C78'
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='C79' then 'DXX_C79'
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='C7A' then 'DXX_C7A'
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='C80' then 'DXX_C80'
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='C81' then 'DXX_C81'
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='C83' then 'DXX_C83'
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='C84' then 'DXX_C84'
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='C85' then 'DXX_C85'
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='C88' then 'DXX_C88'
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='C90' then 'DXX_C90'
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='C91' then 'DXX_C91'
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='C92' then 'DXX_C92'
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='C95' then 'DXX_C95'
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='D05' then 'DXX_D05'
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='D07' then 'DXX_D07'
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='D12' then 'DXX_D12'
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='D25' then 'DXX_D25'
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='D29' then 'DXX_D29'
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='D32' then 'DXX_D32'
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='D33' then 'DXX_D33'
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='D35' then 'DXX_D35'
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='D37' then 'DXX_D37'
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='D43' then 'DXX_D43'
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='D46' then 'DXX_D46'
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='D47' then 'DXX_D47'
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='D48' then 'DXX_D48'
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='D49' then 'DXX_D49'
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='D50' then 'DXX_D50'
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='D51' then 'DXX_D51'
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='D53' then 'DXX_D53'
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='D56' then 'DXX_D56'
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='D57' then 'DXX_D57'
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='D59' then 'DXX_D59'
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='D61' then 'DXX_D61'
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='D62' then 'DXX_D62'
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='D63' then 'DXX_D63'
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='D64' then 'DXX_D64'
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='D68' then 'DXX_D68'
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='D69' then 'DXX_D69'
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='D70' then 'DXX_D70'
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='D72' then 'DXX_D72'
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='D75' then 'DXX_D75'
							when  SUBSTRING(a.ceoa_answer, 1, 3) ='D86' then 'DXX_D86'
